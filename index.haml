              #dropboxImagesButton.language="_IMAGESDROPBOXCHOOSER_"
      #sheetsSideWindow.rightSideWindow
        .mainWindowHeader.language
          _SHEETSTITLE_
          %a.xButton{:onclick => "UI.PageManager.closeRightPage();"}
        .mainWindowContent.singleton
          %p.mainWindowParagraph.language="_SHEETSEXP01_"
          %p.mainWindowParagraph.language="_SHEETSEXP02_"
