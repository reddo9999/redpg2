          %input#chatDiceFaces.chatInput.language{:data => {:placeholderlingo => "_CHATDICEFACES_"}, :type => "number"}
          %input#chatDiceMod.chatInput.language{:data => {:placeholderlingo => "_CHATDICEMOD_"}, :type => "number"}
