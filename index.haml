          .moodButton.language{:onclick=>"UI.Chat.Filters.applyFilter('evening');"}="_MOODEVENING_"
          .moodButton.language{:onclick=>"UI.Chat.Filters.applyFilter('fire');"}="_MOODFIRE_"
          .moodButton.language{:onclick=>"UI.Chat.Filters.applyFilter('trauma');"}="_MOODTRAUMA_"
          .moodButton.language{:onclick=>"UI.Chat.Filters.applyFilter('sepia');"}="_MOODSEPIA_"
        #chatBox
          %p#chatTitle
          %p#chatDescription
          %p.chatInitHelp.language="_CHATHELP01_"
          %p.chatInitHelp.language="_CHATHELP02_"
          %p.chatInitHelp.language="_CHATHELP03_"
          %p.chatInitHelp.language="_CHATHELP04_"
          %p.chatInitHelp.language="_CHATHELP05_"
          %p.chatInitHelp.language="_CHATHELP06_"
          #chatMessages
        #chatScrollDown
        #chatButtonsBox
          %a.chatRightButton.icons-chatMood.language#chatMoodButton{:data => {:titlelingo => "_CHATBUTTONMOOD_"}}
          %a.chatRightButton.icons-chatLeave.language{:data => {:titlelingo => "_CHATBUTTONLEAVE_"}, :onclick => "UI.Chat.leave();"}
          %a.chatRightButton.icons-chatFontPlus.language{:data => {:titlelingo => "_CHATBUTTONFONTPLUS_"}, :onclick => "Application.Config.getConfig('chatfontsize').storeValue(Application.Config.getConfig('chatfontsize').getValue() + 1);"}
          %a.chatRightButton.icons-chatFontMinus.language{:data => {:titlelingo => "_CHATBUTTONFONTMINUS_"}, :onclick => "Application.Config.getConfig('chatfontsize').storeValue(Application.Config.getConfig('chatfontsize').getValue() - 1);"}
