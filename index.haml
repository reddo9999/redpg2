          %a.chatRightButton.icons-chatCombatButton.language#chatCombatButton{:data => {:titlelingo => "_CHATBUTTONCOMBAT_"}}
          %a.chatRightButton.icons-chatLingo.language#chatLingoButton{:data => {:titlelingo => "_CHATBUTTONLINGO_"}}
        #chatMessageStateIcon
        #chatMessageBox
          %input.chatInput#chatMessageInput{:autocomplete => "off"}
        %a.icons-chatSend#chatMessageSendButton.language{:data => {:titlelingo => "_CHATBUTTONSEND_"}}
        #personaDesignerBox
          #personaDesigner
            .language.mainWindowHeader
              _PERSONADESIGNERTITLE_
              %a.xButton#personaDesignerCloseButton
            .mainWindowContent.singleton
              %form#personaDesignerForm
                %input.language#personaDesignerNameInput{:data => {:placeholderlingo => "_PERSONADESIGNERNAME_"}}
                %input.language#personaDesignerAvatarInput{:data => {:placeholderlingo => "_PERSONADESIGNERAVATAR_"}}
