          .diceButton.icons-d8#chatDiceD8{:title => "d8"}
          .diceButton.icons-d10#chatDiceD10{:title => "d10"}
          .diceButton.icons-d12#chatDiceD12{:title => "d12"}
          .diceButton.icons-d20#chatDiceD20{:title => "d20"}
          .diceButton.icons-d100#chatDiceD100{:title => "d100"}
      -# Right Side Windows
      -##### Sheet Permissions
      #sheetPermSideWindow.rightSideWindow
        .language.mainWindowHeader
          _SHEETPERMISSIONSHEADER_
          %a.xButton{:onclick => "UI.PageManager.closeRightPage();"}
        .mainWindowContent.singleton
