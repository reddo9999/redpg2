        .language.mainWindowHeader="_GAMEINVITEDESIGNERTITLE_"
        .mainWindowContent.singleton
          %p.mainWindowParagraph.language="_GAMEINVITEDESIGNEREXP01_"
          %p.mainWindowParagraph.language#gameInviteDesignerGameName="_GAMEINVITEDESIGNEREXP02_"
          %p.mainWindowParagraphError.gameInviteDesignerMessage.language#gameInviteDesigner404="_GAMEINVITEDESIGNERERROR404_"
