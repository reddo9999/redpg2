{
  "compilerOptions": {
    "module": "system",
    "target": "es2015",
    "noImplicitAny": false,
    "sourceMap": true,
    "inlineSourceMap": false,
    "removeComments": true,
    "noImplicitThis": true,
    "inlineSources": true,
    "declaration": true,
    "outFile": "js/Application.js",
    "types": [],
    "moduleResolution": "node"
  },
  "exclude": ["js/**/*"],
  "include": ["app/main.ts"]
}